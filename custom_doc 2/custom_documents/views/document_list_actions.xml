<odoo>

  <!-- Download -->
  <record id="sa_doc_download" model="ir.actions.server">
    <field name="name">Download</field>
    <field name="model_id" ref="model_custom_document"/>
    <field name="binding_model_id" ref="model_custom_document"/>
    <field name="binding_view_types">list</field>
    <field name="state">code</field>
    <field name="code">
records = env['custom.document'].browse(env.context.get('active_ids', []))
action = records.action_menu_download()

<?xml version="1.0" encoding="utf-8"?>
<odoo>
  <record id="view_custom_document_properties_wizard_form" model="ir.ui.view">
    <field name="name">custom.document.properties.wizard.form</field>
    <field name="model">custom.document.properties.wizard</field>
    <field name="arch" type="xml">
      <form string="Info &amp; tags" create="0" edit="0" delete="0">
        <sheet>
          <group>
            <field name="name"/>
            <field name="tag_ids" widget="many2many_tags"/>
            <field name="folder_id"/>
          </group>
        </sheet>
        <footer>
          <button string="Save" type="object" name="action_save" class="btn-primary"/>
          <button string="Cancel" class="btn-secondary" special= "cancel"/>
        </footer>
      </form>
    </field>
  </record>
</odoo>

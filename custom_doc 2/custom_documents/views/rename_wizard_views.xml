<?xml version="1.0" encoding="utf-8"?>
<odoo>
  <record id="view_custom_document_rename_wizard_form" model="ir.ui.view">
    <field name="name">custom.document.rename.wizard.form</field>
    <field name="model">custom.document.rename.wizard</field>
    <field name="arch" type="xml">
      <form string="Rename" create="0" edit="0" delete="0">
        <sheet>
          <group>
            <field name="new_name"/>
          </group>
        </sheet>
        <footer>
          <button string="Apply" type="object" name="action_apply" class="btn-primary"/>
          <button string="Cancel" class="btn-secondary" special="cancel"/>
        </footer>
      </form>
    </field>
  </record>
</odoo>
